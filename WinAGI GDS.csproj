<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>WinAGI.Editor</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>WinAGI.Editor.Program</StartupObject>
    <ApplicationIcon>fWinAGI32.ico</ApplicationIcon>
    <Authors>Andrew Korson</Authors>
    <Company />
    <Copyright>2024</Copyright>
    <PackageIcon>winagi.jpg</PackageIcon>
    <ForceDesignerDPIUnaware>true</ForceDesignerDPIUnaware>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="WinAGI\obj\Release\**" />
    <EmbeddedResource Remove="WinAGI\obj\Release\**" />
    <None Remove="WinAGI\obj\Release\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="RichEdAGI.cs" />
    <Compile Remove="WinAGI\obj\Debug\net5.0\.NETCoreApp,Version=v5.0.AssemblyAttributes.cs" />
    <Compile Remove="WinAGI\obj\Debug\net5.0\WinAGI.AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <COMReference Include="stdole.dll">
      <WrapperTool>tlbimp</WrapperTool>
      <VersionMinor>0</VersionMinor>
      <VersionMajor>2</VersionMajor>
      <Guid>00020430-0000-0000-c000-000000000046</Guid>
      <Lcid>0</Lcid>
      <Isolated>false</Isolated>
    </COMReference>
  </ItemGroup>

  <ItemGroup>
    <None Include="WinAGI\WinAGI.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.OLE.Interop" Version="17.0.32112.339" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="EditorResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>EditorResources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="EditorResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>EditorResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="WinAGI\bin\Debug\net5.0\ref\" />
    <Folder Include="WinAGI\bin\Release\net5.0\ref\" />
    <Folder Include="WinAGI\obj\Debug\net5.0\ref\" />
  </ItemGroup>

</Project>