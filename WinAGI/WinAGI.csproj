<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>WinAGI Library</AssemblyName>
    <Authors>Andrew Korson</Authors>
    <Description>Contains all Library functions to access and modify AGI games as implemented by WinAGI</Description>
    <PackageIcon>winagi.jpg</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Z:\WinAGI GDS Files\WinAGI v2-1-2\Resources\winagi.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
